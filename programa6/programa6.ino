//********** Includes *************************************************************

//********** Variables ************************************************************
const int led1 = 1;
 //*********************************** SETUP*************************************
void setup()
{
  pinMode(led1, OUTPUT);
  
  digitalWrite(led1, LOW);
  delay(400);
  digitalWrite(led1, HIGH);
  delay(400);
  digitalWrite(led1, LOW);
  delay(400);
  digitalWrite(led1, HIGH);
  delay(400);
  digitalWrite(led1, LOW);
  delay(400);
  digitalWrite(led1, HIGH);
  delay(400);
  digitalWrite(led1, LOW);
}
//************************************ LOOP*******************************************
void loop()
{
}
